sync: db/REMOTE-OBJ-CATALOG-000001

batch db
set a 1
set b 2
set c 3
----
sync-data: db/000002.log

flush db
----
sync-data: db/000002.log
close: db/000002.log
create: db/000004.log
sync: db
sync: db/REMOTE-OBJ-CATALOG-000001
sync: db/MANIFEST-000001

batch db
set b 5
set d 7
set e 8
----
sync-data: db/000004.log

flush db
----
sync-data: db/000004.log
close: db/000004.log
reuseForWrite: db/000002.log -> db/000006.log
sync: db
sync: db/REMOTE-OBJ-CATALOG-000001
sync: db/MANIFEST-000001

batch db
set f 9
set g 10
----
sync-data: db/000006.log

