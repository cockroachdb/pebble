# A single policy.

test d-f:lowlatency
a b c d e f g
----
a ->  until d
b ->  until d
c ->  until d
d -> disable-value-separation-by-suffix,no-value-separation until f
e -> disable-value-separation-by-suffix,no-value-separation until f
f -> none
g -> none


# A single encompassing policy.

test a-z:lowlatency
a b c d e z
----
a -> disable-value-separation-by-suffix,no-value-separation until z
b -> disable-value-separation-by-suffix,no-value-separation until z
c -> disable-value-separation-by-suffix,no-value-separation until z
d -> disable-value-separation-by-suffix,no-value-separation until z
e -> disable-value-separation-by-suffix,no-value-separation until z
z -> none

# Abutting policies.

test b-d:lowlatency d-f:latencytolerant f-h:lowlatency
a b c d e f g h i z
----
a ->  until b
b -> disable-value-separation-by-suffix,no-value-separation until d
c -> disable-value-separation-by-suffix,no-value-separation until d
d -> override until f
e -> override until f
f -> disable-value-separation-by-suffix,no-value-separation until h
g -> disable-value-separation-by-suffix,no-value-separation until h
h -> none
i -> none
z -> none

# Gaps between policies.

test b-d:lowlatency h-j:latencytolerant
a b c d e f g h i j k l
----
a ->  until b
b -> disable-value-separation-by-suffix,no-value-separation until d
c -> disable-value-separation-by-suffix,no-value-separation until d
d ->  until h
e ->  until h
f ->  until h
g ->  until h
h -> override until j
i -> override until j
j -> none
k -> none
l -> none
