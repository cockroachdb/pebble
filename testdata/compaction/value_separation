# More tests compacting tables with mixed value separation policies.
# After changing span policies, blob references should no longer
# be preserved.
define value-separation=(enabled, min-size=1, max-ref-depth=3, rw-min-age=0, garbage-ratios=1.0:1.0)
----

# With these span policies, only values for keys n-z will be separated.
set-span-policies
a,f val-sep-minimum-size=100
f,o val-sep-minimum-size=0
----

batch
set a aa
set b bb
set f ff
set g gg
set o oo
set p pp
----

flush
----
L0.0:
  000005:[a#10,SET-b#11,SET] seqnums:[#10-#11] points:[a#10,SET-b#11,SET] size:698
  000006:[f#12,SET-g#13,SET] seqnums:[#12-#13] points:[f#12,SET-g#13,SET] size:668
  000007:[o#14,SET-p#15,SET] seqnums:[#14-#15] points:[o#14,SET-p#15,SET] size:780 blobrefs:[(B000008: 4); depth:1]
Blob files:
  B000008 physical:{000008 size:[94 (94B)] vals:[4 (4B)]}

compact a-z
----
L6:
  000009:[a#0,SET-b#0,SET] seqnums:[#0-#0] points:[a#0,SET-b#0,SET] size:693
  000010:[f#0,SET-g#0,SET] seqnums:[#0-#0] points:[f#0,SET-g#0,SET] size:663
  000011:[o#0,SET-p#0,SET] seqnums:[#0-#0] points:[o#0,SET-p#0,SET] size:779 blobrefs:[(B000008: 4); depth:1]
Blob files:
  B000008 physical:{000008 size:[94 (94B)] vals:[4 (4B)]}

# Adjust the span policies. The first span policy is truncated to only cover d-f.
# The second span policy's minimum size changes from 0 to 10 bytes.
set-span-policies
d,f val-sep-minimum-size=100
f,o val-sep-minimum-size=10
----


batch
set b bb
set f ff
set o oo
----

flush
----
L0.0:
  000013:[b#16,SET-b#16,SET] seqnums:[#16-#16] points:[b#16,SET-b#16,SET] size:779 blobrefs:[(B000014: 2); depth:1]
  000015:[f#17,SET-f#17,SET] seqnums:[#17-#17] points:[f#17,SET-f#17,SET] size:683
  000016:[o#18,SET-o#18,SET] seqnums:[#18-#18] points:[o#18,SET-o#18,SET] size:779 blobrefs:[(B000017: 2); depth:1]
