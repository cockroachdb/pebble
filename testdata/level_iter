# levelIter trims lower/upper bound in the options passed to sstables.
load a
----
[,]

load b lower=aa upper=bb
----
[aa,]

load b lower=aa upper=c
----
[aa,]

load c lower=b upper=d
----
[,d]

load c lower=b upper=e
----
[,]

iter lower=b
seek-ge b
----
b#2,SET:2

iter lower=c
seek-ge c
----
c#3,SET:3

iter
set-bounds lower=b
seek-ge b
----
b#2,SET:2

iter
set-bounds lower=c
seek-ge c
----
c#3,SET:3

