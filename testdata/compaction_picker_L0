  001621:[b#1261,MERGE-b#1261,MERGE]
  001603:[d#1248,DEL-d#1248,DEL]
  001609:[e#1253,DEL-e#1253,DEL]
6:
  001615:[a#1254,RANGEDEL-c#72057594037927935,RANGEDEL]
  001619:[c#0,SET-c#0,SET]

