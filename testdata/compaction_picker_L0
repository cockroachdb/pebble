# 3 L0 files (1 overlap, 1 intra-L0 compacting). Should avoid the compacting
# file.

define
L0
   000100:i.SET.101-p.SET.102
   000110:j.SET.111-q.SET.112
   000120:r.SET.113-s.SET.114
