# The del-range tests are specific to the comparer.

define default
del-range a c
del-range e g
----

overlaps
a-b
b-c
c-d
c-e
f-h
g-h
----
true
true
false
true
true
false

define reverse
del-range d b
----

overlaps
c-b
b-a
e-d
----
true
false
true

define default
set b 1
----

overlaps
a#2,RANGEDEL-b#inf,RANGEDEL:
----
true

define default
del-range b c
----

overlaps
a#2,RANGEDEL-b#inf,RANGEDEL:
----
true

define default
del-range a f
del-range b c
----

overlaps
d#2,RANGEDEL-e#inf,RANGEDEL:
----
true

define default
range-key-set a f 1 val1
range-key-set b c 2 val2
----

overlaps
d-e
----
true

define default
range-key-set a c 1 val1
----

overlaps
a-c
b-c
a#2,RANGEDEL-b#inf,RANGEDEL:
d-e
----
true
true
