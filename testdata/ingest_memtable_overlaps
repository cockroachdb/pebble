# The del-range tests are specific to the comparer.

define default
del-range a c
del-range e g
----

overlaps
a-b
b-c
c-d
c-e
f-h
g-h
----
true
true
false
true
true
false

define reverse
del-range d b
----

overlaps
c-b
b-a
e-d
----
true
false
true

define default
set b 1
----

overlaps
a.RANGEDEL.2-b.RANGEDEL.72057594037927935
----
false

define default
del-range b c
----

overlaps
a.RANGEDEL.2-b.RANGEDEL.72057594037927935
----
false
