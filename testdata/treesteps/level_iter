# Test treesteps recording for levelIter SeekGE operation.
# This test creates a DB with multiple sstables in L6 and performs a SeekGE
# operation while recording the operation steps with treesteps.

define
L1
  a#10,SET:va
  b#10,SET:vb
  c#10,SET:vc
L1
  d#5,SET:vd
  d@10#5,SET:vd10
  d@20#5,SET:vd20
  e#5,SET:ve
  f#5,SET:vf
L1
  g#1,SET:vg
  h#1,SET:vh
  i#1,SET:vi
----
L1:
  000004:[a#10,SET-c#10,SET] seqnums:[#10-#10] points:[a#10,SET-c#10,SET] size:700
  000005:[d#5,SET-f#5,SET] seqnums:[#5-#5] points:[d#5,SET-f#5,SET] size:831
  000006:[g#1,SET-i#1,SET] seqnums:[#1-#1] points:[g#1,SET-i#1,SET] size:697

level-iter depth=1
seek-ge b
next
next
next
next
next
next
next
----
