batch
set a 1
set b 2
----

compact a-b
----
6:
  000005:[a#1,SET-b#2,SET]

batch
set c 3
set d 4
----

compact c-d
----
6:
  000005:[a#1,SET-b#2,SET]
  000007:[c#3,SET-d#4,SET]

batch
set b 5
set c 6
----

compact a-d
----
6:
  000010:[a#0,SET-d#0,SET]

