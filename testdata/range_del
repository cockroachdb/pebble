  000004:[a#2,RANGEDEL-b#72057594037927935,RANGEDEL]
  000005:[b#2,RANGEDEL-c#72057594037927935,RANGEDEL]
  000006:[c#2,RANGEDEL-d#72057594037927935,RANGEDEL]
