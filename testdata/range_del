# 1 memtable.

define
mem
  a.SET.10:b
  a.SET.12:c
  a.SET.14:d
  b.MERGE.10:b
  b.MERGE.12:c
  b.MERGE.14:d
  b.RANGEDEL.15:c
  b.MERGE.16:e
  c.SET.10:b
  c.SET.12:c
  c.SET.14:d
----
mem: 1

