# Range deletions with varying overlap.
define
L0
  a.SET.13:4
  b.SET.13:4
  d.SET.13:4
  c.RANGEDEL.13:d
L1
  a.SET.12:3
  d.SET.12:3
  b.RANGEDEL.12:d
L2
  d.SET.11:2
  a.RANGEDEL.11:d
L3
  a.SET.10:1
  b.SET.10:1
  c.SET.10:1
  d.SET.10:1
----
mem: 1
L0.0:
