# Overlapping range deletions in different memtables. Note that the
# range tombstones are not fragmented in this case.

define
mem
  a.SET.1:1
  b.SET.1:1
  c.SET.1:1
  d.SET.1:1
mem
  a.SET.3:2
  b.SET.3:2
  c.SET.3:2
  d.SET.3:2
  a.RANGEDEL.2:d
mem
  a.SET.5:3
  b.SET.5:3
  c.SET.5:3
  d.SET.5:3
  b.RANGEDEL.4:d
mem
  a.SET.7:4
  b.SET.7:4
  c.SET.7:4
  d.SET.7:4
  c.RANGEDEL.4:d
----
