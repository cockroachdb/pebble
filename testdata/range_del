# 1 memtable.

define
mem
  a.SET.1:b
  a.SET.3:c
  a.SET.5:d
  b.MERGE.1:b
  b.MERGE.3:c
  b.MERGE.5:d
  b.RANGEDEL.6:c
  b.MERGE.7:e
  c.SET.1:b
  c.SET.3:c
  c.SET.5:d
