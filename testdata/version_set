# Convert 000002 to a virtual table.
apply
  del-table:   L2 000002
  add-table:   L2 000003(000002):[e#1,SET-h#1,SET]
  add-backing: 000002
----
applied:
  last-seq-num:  99
  del-table:     L2 000002
