# Ensure that we pause at (and return) rangedel start points correctly.

define
a#4,RANGEDEL:b
a#5,SET:foo
b#3,RANGEDEL:c
b#6,SET:foo
b#4,DEL:
c#7,SET:bar
c#5,SET:foo
----

iter
seek-ge b
next
next
----
b#inf,RANGEDEL:
b#6,SET:foo
c#7,SET:bar

# More rangedel elision tests

define
a#4,RANGEDEL:b
a#5,SET:foo
b#4,RANGEDEL:c
b#3,SET:foo
b#2,DEL:
c#7,SET:bar
c#5,SET:foo
----

iter
seek-ge a
next
next
next
next
----
a#inf,RANGEDEL:
a#5,SET:foo
b#inf,RANGEDEL:
c#7,SET:bar
.
