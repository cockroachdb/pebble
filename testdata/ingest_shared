# Reverse iteration test with masking.

iter mask-filter mask-suffix=@6
last
prev
prev
prev
prev
prev
prev
prev
----
l: (2, .)
i@5: (baz, [g-j) @4=value UPDATED)
g: (bar3, [g-j) @4=value)
f: (bar3, . UPDATED)
e: (bar, .)
d: (foo, .)
a: (1, .)
.

# Range del tests.

reset
----

switch 1
----
ok

batch
set a@3 o
set b@5 foo
set c@6 bar
set e baz
----

flush
----

compact a-z
----
ok

batch
del-range b d
----

flush
----

batch
set a@3 abc
set b@7 notdeleted
set c@9 foobar
----

flush
----

lsm
----
L0.1:
  000009:[a@3#15,SET-c@9#17,SET]
L0.0:
  000007:[b#14,RANGEDEL-d#inf,RANGEDEL]
L6:
  000005:[a@3#10,SET-e#13,SET]

iter
first
next
next
next
next
----
