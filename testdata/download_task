# This file tests download span task functionality.
# Backings >= 100 are external.

define
L0:
  1(101):[a#1,SET-c#1,SET]
  2(102):[d#1,SET-f#1,SET]
  3(103):[g#1,SET-i#1,SET]
L1:
  4(104):[b#1,SET-d#1,SET]
  5(105):[e#1,SET-e#1,SET]
  6(106):[f#1,SET-j#1,SET]
L2:
  7(107):[a#1,SET-e#1,SET]
  8(108):[h#1,SET-i#1,SET]
----

# Simple case, none of the tables are already compacting.
new-task start=a end=z
----
cursor 0: level=0 key="a" seqNum=0

try-launch
----
launching download for 000001
cursor 0: level=0 key="a" seqNum=1

try-launch
----
launching download for 000002
cursor 0: level=0 key="d" seqNum=1

try-launch
----
launching download for 000003
cursor 0: level=0 key="g" seqNum=1

try-launch
----
launching download for 000004
cursor 0: level=1 key="b" seqNum=1

try-launch
----
launching download for 000005
cursor 0: level=1 key="e" seqNum=1

try-launch
----
launching download for 000006
cursor 0: level=1 key="f" seqNum=1

try-launch
----
launching download for 000007
cursor 0: level=2 key="a" seqNum=1

try-launch
----
launching download for 000008
cursor 0: level=2 key="h" seqNum=1

try-launch
----
task completed

# Set some already-compacting tables.
set-compacting
2 5 6
----

new-task start=a end=z
----
cursor 0: level=0 key="a" seqNum=0

try-launch
----
launching download for 000001
cursor 0: level=0 key="a" seqNum=1

# Now we encounter file 2; we leave cursor 0 there and advance with a new
# cursor.
try-launch
----
launching download for 000003
cursor 0: level=0 key="d" seqNum=0
cursor 1: level=0 key="g" seqNum=1

try-launch
----
launching download for 000004
cursor 0: level=0 key="d" seqNum=0
cursor 1: level=1 key="b" seqNum=1

# We encountered file 5 and the task will now wait for one of these compactions
# to finish.
try-launch
----
cursor 0: level=0 key="d" seqNum=0
cursor 1: level=1 key="e" seqNum=0

# If the concurrency is increased, a new cursor will be used to advance. We find
# file 5 and are still stuck.
try-launch max-concurrent-downloads=2
----
cursor 0: level=0 key="d" seqNum=0
cursor 1: level=1 key="e" seqNum=0
cursor 2: level=1 key="f" seqNum=0

# With concurrency 3 we can launch new downloads.
try-launch max-concurrent-downloads=3
----
launching download for 000007
cursor 0: level=0 key="d" seqNum=0
cursor 1: level=1 key="e" seqNum=0
cursor 2: level=1 key="f" seqNum=0
cursor 3: level=2 key="a" seqNum=1

# Update version as if files 1 through 5 were downloaded and 6 and 7 are still
# downloading.
define
L0:
  1:[a#1,SET-c#1,SET]
  2:[d#1,SET-f#1,SET]
  3:[g#1,SET-i#1,SET]
L1:
  4:[b#1,SET-d#1,SET]
  5:[e#1,SET-e#1,SET]
  6(106):[f#1,SET-j#1,SET]
L2:
  7(107):[a#1,SET-e#1,SET]
  8(108):[h#1,SET-i#1,SET]
----

set-compacting
6 7
----

try-launch
----
launching download for 000008
cursor 0: level=1 key="f" seqNum=0
cursor 1: level=2 key="h" seqNum=1

try-launch
----
cursor 0: level=1 key="f" seqNum=0
cursor 1: level=7 key="a" seqNum=0

# Update version as if all files have been downloaded.
define
L0:
  1:[a#1,SET-c#1,SET]
  2:[d#1,SET-f#1,SET]
  3:[g#1,SET-i#1,SET]
L1:
  4:[b#1,SET-d#1,SET]
  5:[e#1,SET-e#1,SET]
  6:[f#1,SET-j#1,SET]
L2:
  7:[a#1,SET-e#1,SET]
  8:[h#1,SET-i#1,SET]
----

try-launch
----
task completed
