  000007(000005):[a#10,SET-c#12,SET]
  000008(000005):[e#14,SET-j#19,SET]
L6:
  000006:[z#21,SET-z#21,SET]

build ext3
set zz zz
----

ingest-and-excise ext3 excise=g-h
----

# 7, 10, 11 should have the same file backing struct.
