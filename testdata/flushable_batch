# Test a LogData interleaved among batch data. The LogData entry should not be
# assigned a sequence number or appear within the iterator output.

define
b#1,SET:
a#1,SET:
c#1,SET:
b#2,LOGDATA:
1#3,RANGEDEL:
a#2,SET:
2#4,RANGEKEYSET:
1#3,RANGEKEYDEL:
c#2,MERGE:
b#3,DEL:
2#3,RANGEKEYUNSET:
----

dump seq=0
----
a#4,SET:2
a#1,SET:1
b#8,DEL:
b#0,SET:1
c#7,MERGE:2
c#2,SET:1
1-3:{(#3,RANGEDEL)}
1-2:{(#6,RANGEKEYDEL)}
2-3:{(#9,RANGEKEYUNSET,3) (#6,RANGEKEYDEL) (#5,RANGEKEYSET,4,4)}
3-4:{(#5,RANGEKEYSET,4,4)}
