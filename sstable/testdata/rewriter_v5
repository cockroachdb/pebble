# Rewrite a table that contain only range keys.

build block-size=1 index-block-size=1 filter comparer=testkeys
EncodeSpan: a-b:{(#1,RANGEKEYSET,@000)}
EncodeSpan: b-c:{(#1,RANGEKEYSET,@000)}
EncodeSpan: c-d:{(#1,RANGEKEYSET,@000)}
----
rangekey: [a#1,RANGEKEYSET-d#inf,RANGEKEYSET]
seqnums:  [1-1]

scan-range-key
----
a-b:{(#1,RANGEKEYSET,@000)}
b-c:{(#1,RANGEKEYSET,@000)}
c-d:{(#1,RANGEKEYSET,@000)}

rewrite from=@000 to=@123 block-size=1 index-block-size=1 filter comparer=testkeys
----
rangekey: [a#1,RANGEKEYSET-d#inf,RANGEKEYSET]
seqnums:  [1-1]

scan-range-key
----
a-b:{(#1,RANGEKEYSET,@123)}
b-c:{(#1,RANGEKEYSET,@123)}
c-d:{(#1,RANGEKEYSET,@123)}
