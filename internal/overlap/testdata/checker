define t1
points:
  b.SET.1
  d.SET.1
  u.SET.1
----

overlap t1
[a, b)
[a, b]
[b1, b2]
[b, c]
[d, e]
[e, u)
[e, u]
[u, w]
[v, w]
----
[a, b): no overlap
[a, b]: possible overlap
[b1, b2]: no overlap
[b, c]: possible overlap
[d, e]: possible overlap
[e, u): no overlap
[e, u]: possible overlap
[u, w]: possible overlap
[v, w]: no overlap

define t2
range-dels:
  b-e:{#1,RANGEDEL}
  h-k:{#1,RANGEDEL}
----

overlap t2
[a, b)
[a, b]
[d, f]
[e, f]
[e, h)
[e, h]
[j, l)
[k, l]
----
[a, b): no overlap
[a, b]: possible overlap
[d, f]: possible overlap
[e, f]: no overlap
[e, h): no overlap
[e, h]: possible overlap
[j, l): possible overlap
[k, l]: no overlap

define t3
range-keys:
  b-e:{#1,RANGEKEYDEL}
  h-k:{#1,RANGEKEYDEL}
----

overlap t3
[a, b)
[a, b]
[d, f]
[e, f]
[e, h)
[e, h]
[j, l)
[k, l]
----
