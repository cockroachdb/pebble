open test
----

list test
----

batch
add 1 10 100
----
create: test/SHARED-CATALOG-000001
sync: test/SHARED-CATALOG-000001
create: test/marker.shared-catalog.000001.SHARED-CATALOG-000001
close: test/marker.shared-catalog.000001.SHARED-CATALOG-000001
sync: test
sync: test/SHARED-CATALOG-000001

